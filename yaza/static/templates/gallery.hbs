{{#if this}}
<ul class="thumbnails gallery picker">
  {{#each this}}
      {{#each this}}
        <li>
          <div class="thumbnail">
            <img src="{{this}}" alt="{{this}}"/>
          </div>
        </li>
      {{/each}}
  {{/each}}
</ul>
{{else}}
  <div class="alert alert-info">
    {{t "You havn't uploaded any images yet!"}}
  </div>
{{/if}}
